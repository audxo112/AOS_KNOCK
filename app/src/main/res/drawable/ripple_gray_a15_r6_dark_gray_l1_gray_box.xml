<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="@color/colorEffectGrayA15">

    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <corners
                android:radius="6dp"/>

            <stroke
                android:width="1dp"
                android:color="@color/colorLineGray"
                android:dashGap="6dp"
                android:dashWidth="12dp"/>

            <solid
                android:color="@color/colorBackgroundDarkGray"/>
        </shape>
    </item>
</ripple>